<?xml version="1.0" encoding="UTF-8"?>
<map>
    <!--Author：liyu-->
    <!--忘记密码功能-->
    <!--忘记密码-->
    <locator type="ID" timeOut="3" value="forgot_password_tv">forgot_password</locator>
    <!--注册过的手机号-->
    <locator type="XPATH" timeOut="3" value="//*[@class='android.widget.RelativeLayout'][contains(@index,1)]">mobile_code</locator>
    <!--获取验证码-->
    <locator type="ID" timeOut="3" value="send_msg_tv">TestGetCode</locator>
    <!--验证码-->
    <locator type="XPATH" timeOut="3" value="//*[@class='android.widget.LinearLayout'][contains(@index,0)]">TestCode</locator>
    <!--下一步-->
    <locator type="ID" timeOut="3" value="next_bt">next</locator>

    <!--忘记密码详细信息页面-->
    <!--忘记密码-->
    <locator type="XPATH" timeOut="3" value="//*[@class='android.widget.TextView'][contains(@text,'忘记密码')]">password_title</locator>
    <!--新密码-->
    <locator type="XPATH" timeOut="3" value="//*[@resource-id='com.iscs.mobilewcs:id/new_password_et'][contains(@index,0)]">new_password</locator>
    <!--确认新密码-->
    <locator type="XPATH" timeOut="3" value="//*[@resource-id='com.iscs.mobilewcs:id/new_password_again_et'][contains(@index,0)]">confirm_password</locator>
    <!--提交-->
    <locator type="ID" timeOut="3" value="submit_bt">submit</locator>
    <!--修改密码成功确认按钮-->
    <locator type="ID" timeOut="3" value="button1">confirm</locator>
</map>