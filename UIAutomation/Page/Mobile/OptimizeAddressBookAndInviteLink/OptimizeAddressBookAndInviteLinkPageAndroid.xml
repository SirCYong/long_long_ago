<map>
    <!--选择好友-->
    <locator type="ID" timeOut="3" value="btn_invite_contact">choice</locator>
    <!--搜索-->
    <locator type="ID" timeOut="3" value="custom_btn">search</locator>
    <!--用户名-->
    <locator type="ID" timeOut="3" value="cet_person">name</locator>
    <!--任务权限分配-->
    <locator type="ID" timeOut="3" value="task_permission_container">task</locator>
    <!--选择权限-->
    <locator type="XPATH" timeOut="3" value="//*[@resource-id='com.iscs.mobilewcs:id/tv_name'and @text='入库卸货']">permission</locator>
    <!--复制权限-->
    <locator type="ID" timeOut="3" value="btn_copy">copy</locator>
    <!--邀请状态-->
    <locator type="xpath" timeOut="3" value="//*[@resource-id='com.iscs.mobilewcs:id/btn_type_content' and @text='邀请']">invite</locator>
    <!--已注册状态-->
    <locator type="xpath" timeOut="3" value="//*[@resource-id='com.iscs.mobilewcs:id/btn_type_content' and @text='已注册']">register</locator>
    <!--无需分配状态-->
    <locator type="XPATH" timeOut="3" value="//*[@resource-id='com.iscs.mobilewcs:id/btn_type_content' and @text ='无需分配']">allocate</locator>
    <!--认证中-->
    <locator type="XPATH" timeOut="3" value="//*[@resource-id='com.iscs.mobilewcs:id/btn_type_content' and @text ='认证中']">certification</locator>
    <!--选择状态-->
    <locator type="XPATH" timeOut="3" value="//*[@resource-id='com.iscs.mobilewcs:id/btn_type_content' and @text ='选择']">select</locator>
</map>

